@Model List<Item>
    @{
        ViewData["Title"] = "Index";
    }

    <h1>Wishlist</h1>

    <a asp-action="create">Add item</a>
    <ul>
        @foreach (WishList.Models.Item item in Model)
        {
            <li>@item.Description</li>
            <a asp-action="delete" asp-route-id="@item.Id">delete</a>
        }
    </ul>
